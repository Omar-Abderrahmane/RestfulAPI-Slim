        "tuupola/slim-basic-auth": "2.0",
        "illuminate/database": "*",
        "illuminate/pagination": "*",
        "vlucas/phpdotenv": "^4.3",
        "monolog/monolog": "*",
        "tuupola/slim-jwt-auth":"*",
        "tuupola/cors-middleware": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
